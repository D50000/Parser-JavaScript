{
  "name": "which-browser-js",
  "version": "0.0.1",
  "description": "Browser sniffing tool",
  "main": "index.js",
  "scripts": {
    "test": "lab -a code -v",
    "build": "babel src --presets babel-preset-es2015 --out-dir dist/src && babel data --presets babel-preset-es2015 --out-dir dist/data",
    "update-browser-ids": "node ./bin/update-browser-ids",
    "update-chrome": "node ./bin/update-chrome",
    "update-profiles": "node ./bin/update-profiles",
    "update-applications": "node ./bin/update-applications"
  },
  "engines": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariotsi/WhichBrowser-js.git"
  },
  "keywords": [
    "browser",
    "sniffing",
    "user-agent",
    "useragent",
    "node"
  ],
  "author": "Simone Mariotti",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6",
    "babel-preset-es2015": "6",
    "code": "^4.1.0",
    "codecov": "^2.2.0",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.8.0",
    "istanbul": "^0.4.5",
    "lab": "^14.1.0",
    "request": "^2.81.0",
    "sinon": "^2.3.5"
  }
}
