{
  "name": "which-browser-js",
  "version": "0.0.1",
  "description": "Browser sniffing tool",
  "main": "index.js",
  "scripts": {
    "test": "lab -a code -v",
    "precommit": "lint-staged",
    "update-browser-ids": "node ./bin/update-browser-ids",
    "update-chrome": "node ./bin/update-chrome",
    "update-profiles": "node ./bin/update-profiles",
    "update-applications": "node ./bin/update-applications",
    "update-models": "node ./bin/update-models"
  },
  "engines": {
    "node": "6.x.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mariotsi/WhichBrowser-js.git"
  },
  "keywords": [
    "browser",
    "sniffing",
    "user-agent",
    "useragent",
    "node"
  ],
  "author": "Simone Mariotti",
  "license": "MIT",
  "devDependencies": {
    "code": "^4.1.0",
    "codecov": "^2.2.0",
    "eslint": "^4.1.1",
    "eslint-config-google": "^0.8.0",
    "istanbul": "^0.4.5",
    "husky": "^0.14.1",
    "lab": "^14.1.0",
    "lint-staged": "^4.0.0",
    "prettier": "^1.5.2",
    "request": "^2.81.0",
    "sinon": "^2.3.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --print-width 117 --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  }
}
